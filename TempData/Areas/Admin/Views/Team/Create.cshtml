@model TeamCreateVM
@{
    ViewData["Title"] = "Create";
}

<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Position"></label>
        <input asp-for="Position" class="form-control" id="exampleInputEmail1" placeholder="Enter position">
        <span asp-validation-for="Position" class="form-text text-muted"></span>
    </div>
    <div class="form-group">
        <label asp-for="Image"></label>
        <input asp-for="Image" class="form-control" id="exampleInputEmail1" placeholder="Enter photo">
        <span asp-validation-for="Image" class="form-text text-muted"></span>
    </div>
    
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

